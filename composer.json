{
    "name": "rollerworks/search-symfony-validator",
    "description": "Symfony validator extension for RollerworksSearch",
    "keywords": ["rollerworks", "search", "validation", "symfony"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollercapes.net"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/Rollerworks/rollerworks-search-symfony-validator/contributors"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "rollerworks/search": ">1.0.0-beta4@dev",
        "symfony/validator": "~2.3"
    },
    "require-dev": {
        "phpspec/prophecy": "^1.3.1"
    },
    "autoload": {
        "psr-4": {
            "Rollerworks\\Component\\Search\\Extension\\Symfony\\Validator\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
