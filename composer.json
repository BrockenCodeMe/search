{
    "name": "rollerworks/search",
    "description": "Advanced search-building framework",
    "keywords": ["rollerworks", "filter", "search"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollercapes.net"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/Rollerworks/RollerworksSearch/contributors"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/options-resolver": "~2.1",
        "symfony/property-access": "~2.2",
        "symfony/intl": "~2.3",
        "symfony/yaml": "~2.1",
        "symfony/validator": "~2.1",
        "seld/jsonlint": "1.*"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.1@dev",
        "phpspec/prophecy": "~1.1@dev",
        "rollerworks/search-jms-metadata": "1.*@dev"
    },
    "autoload": {
        "psr-4": { "Rollerworks\\Component\\Search\\": "src/" }
    },
    "autoload-dev": {
        "psr-0": { "Rollerworks\\Component\\Search\\Tests\\": "tests/" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
