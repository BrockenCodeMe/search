{
    "name": "rollerworks/search-bundle",
    "description": "Rollerworks Search Bundle",
    "keywords": ["filter", "search", "rollerworks", "symfony", "bundle"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollercapes.net"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "rollerworks/search-symfony-di": "~1.0@dev,>1.0.0-beta5",
        "symfony/framework-bundle": "~2.3",
        "symfony/finder": "~2.3"
    },
    "require-dev": {
        "rollerworks/search-jms-metadata": "~1.0@dev,>=1.0.0-beta2",
        "rollerworks/cache-bundle": "1.0.*@dev",
        "phpunit/phpunit": "~4.4",
        "matthiasnoback/symfony-dependency-injection-test": "^0.7.1",
        "matthiasnoback/symfony-service-definition-validator": "^1.2.2"
    },
    "suggest": {
        "rollerworks/cache-bundle": "Session based caching for increased performance"
    },
    "autoload": {
        "psr-4": {
            "Rollerworks\\Bundle\\SearchBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rollerworks\\Bundle\\SearchBundle\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
