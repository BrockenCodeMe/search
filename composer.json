{
    "name": "rollerworks/search-doctrine-orm",
    "description": "Doctrine ORM extension for RollerworksSearch",
    "keywords": ["rollerworks", "search", "doctrine", "orm"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollercapes.net"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/Rollerworks/rollerworks-search-doctrine-orm/contributors"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "rollerworks/search": ">=1.0.0-beta2@dev",
        "rollerworks/search-doctrine-dbal": "1.0.0-beta3@dev",
        "doctrine/orm": "~2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7"
    },
    "autoload": {
        "psr-4": {
            "Rollerworks\\Component\\Search\\Doctrine\\Orm\\": "src/",
            "Rollerworks\\Component\\Search\\Extension\\Doctrine\\Orm\\": "src/Extension/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "Doctrine\\Tests\\": "vendor/doctrine/orm/tests"
        },
        "psr-4": {
            "Rollerworks\\Component\\Search\\Tests\\Mocks\\": [
                "vendor/rollerworks/search-doctrine-dbal/tests/Mocks/"
            ],
            "Rollerworks\\Component\\Search\\Tests\\Fixtures\\": [
                "tests/Fixtures/",
                "vendor/rollerworks/search-doctrine-dbal/tests/Fixtures/"
            ],
            "Rollerworks\\Component\\Search\\Tests\\Doctrine\\Dbal\\": "vendor/rollerworks/search-doctrine-dbal/tests/",
            "Rollerworks\\Component\\Search\\Tests\\Doctrine\\Orm\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
