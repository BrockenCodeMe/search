{
    "name": "rollerworks/search-elasticsearch",
    "description": "ElasticSearch (Elastica) extension for RollerworksSearch",
    "keywords": ["rollerworks", "search", "doctrine", "dbal"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollercapes.net"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/Rollerworks/search-elasticsearch/contributors"
        }
    ],
    "require": {
        "php": "^7.1",
        "rollerworks/search": "^2.0@dev,>=2.0-ALPHA5",
        "ruflin/elastica": "^5.1.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.3.6",
        "symfony/var-dumper": "^3.3.6"
    },
    "autoload": {
        "psr-4": {
            "Rollerworks\\Component\\Search\\ElasticSearch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rollerworks\\Component\\Search\\Tests\\ElasticSearch\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
